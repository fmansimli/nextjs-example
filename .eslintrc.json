{
  "extends": ["next", "eslint:recommended", "prettier"],
  "rules": {
    "@typescipt-eslint/no-explicit-any": "off",
    "react-hooks/exhaustive-deps": "off",
    "no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ],
    "no-undef": "off",
    "jsx-a11y/alt-text": "off"
  }
}
